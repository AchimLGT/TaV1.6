local ReplicatedStorage = game:GetService("ReplicatedStorage")
local SayMessageRequest = ReplicatedStorage:WaitForChild("DefaultChatSystemChatEvents"):WaitForChild("SayMessageRequest")

local function sendMessage(startIndex)
    local currentMessageIndex = startIndex
    while currentMessageIndex <= #messages do
        if not isPaused then
            SayMessageRequest:FireServer(messages[currentMessageIndex], "All")
            wait(1)
            currentMessageIndex = currentMessageIndex + 1
        else
            wait(0.1)
        end
    end
end

startButton.MouseButton1Click:Connect(function()
    local messageIndex = tonumber(textBox.Text)
    if messageIndex and messageIndex >= 1 and messageIndex <= #messages then
        currentCoroutine = coroutine.create(sendMessage)
        coroutine.resume(currentCoroutine, messageIndex)
    else
        print("Geçersiz mesaj numarası")
    end
end)

stopButton.MouseButton1Click:Connect(function()
    isPaused = true
end)

resumeButton.MouseButton1Click:Connect(function()
    isPaused = false
end)
